<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.divingWeb.elememts.StockProducto" table="stock">

        <id name="id">
            <generator class="native" />
        </id>

        <property name="idProducto" />
        <property name="cantidad" />
        <property name="precio" />

    </class>
    
    <sql-query name="call_sp_cargaStock">
		<return alias="stock" class="com.divingWeb.elememts.StockProducto"/>
		<![CDATA[CALL sp_cargaStock(:idProducto,:cantidad,:precio,@resp)]]>
    </sql-query>

</hibernate-mapping>
